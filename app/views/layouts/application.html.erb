<!DOCTYPE html>
<html>
<head>
  <title>Musette</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyBS-sd3MnCwWUtx_jvAUQy8KuCBXU_CiSE&libraries=geometry,places"%>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>">
        <%= value %>
      </div>
    <% end %>

    <%= yield %>
  </div>

  <div id="maps-placeholder"/>

</body>
</html>
